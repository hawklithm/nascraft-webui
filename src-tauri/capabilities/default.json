{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "os:default",
    "fs:default",
    {
      "identifier": "fs:scope",
      "allow": [{ "path": "$APPCONFIG/**" },{"path":"$APPDATA/**"},{"path":"$APPLOG/**"},{"path":"$APPCACHE/**"}]
    },
    "dialog:allow-open",
    "dialog:default",
    "fs:allow-appdata-write",
    "fs:allow-watch"
  ]
}
