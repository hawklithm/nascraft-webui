{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "default",
  "windows": ["main"],
  "description": "enables default permissions",
  "local": true,
  "permissions": [
    "core:default",
    "core:app:default",
    "core:path:default",
    "core:path:allow-resolve-directory",
    "os:default",
    "dialog:default",
    "dialog:allow-open",
    "http:default",
    "http:allow-fetch",
    "fs:default",
    "photo:default",
    "photo:allow-checkAndRequestPermissions",
    "photo:allow-get-album-photos",
    "photo:allow-get-photo-thumbnail",
    "photo:allow-read-photo-data",
    "photo:allow-openAppSettings",
    {
      "identifier": "fs:allow-read-file",
      "allow": [
        { "path": "$APPCONFIG/**" },
        { "path": "$APPDATA/**" },
        { "path": "$APPLOG/**" },
        { "path": "$APPCACHE/**" },
        { "path": "$HOME_DIR/**" }
      ]
    },
    {
      "identifier": "fs:allow-read-text-file",
      "allow": [
        { "path": "$APPCONFIG/**" },
        { "path": "$APPDATA/**" },
        { "path": "$APPLOG/**" },
        { "path": "$APPCACHE/**" },
        { "path": "$HOME_DIR/**" }
      ]
    },
    {
      "identifier": "fs:allow-read",
      "allow": [
        { "path": "$APPCONFIG/**" },
        { "path": "$APPDATA/**" },
        { "path": "$APPLOG/**" },
        { "path": "$APPCACHE/**" },
        { "path": "$HOME_DIR/**" }
      ]
    },
    {
      "identifier": "fs:allow-exists",
      "allow": [
        { "path": "$APPCONFIG/**" },
        { "path": "$APPDATA/**" },
        { "path": "$APPLOG/**" },
        { "path": "$APPCACHE/**" },
        { "path": "$HOME_DIR/**" }
      ]
    },
    {
      "identifier": "fs:allow-mkdir",
      "allow": [
        { "path": "$APPCONFIG/**" },
        { "path": "$APPDATA/**" },
        { "path": "$APPLOG/**" },
        { "path": "$APPCACHE/**" }
      ]
    },
    {
      "identifier": "fs:allow-write-file",
      "allow": [
        { "path": "$APPCONFIG/**" },
        { "path": "$APPDATA/**" },
        { "path": "$APPLOG/**" },
        { "path": "$APPCACHE/**" }
      ]
    },
    {
      "identifier": "fs:allow-write-text-file",
      "allow": [
        { "path": "$APPCONFIG/**" },
        { "path": "$APPDATA/**" },
        { "path": "$APPLOG/**" },
        { "path": "$APPCACHE/**" }
      ]
    },
    {
      "identifier": "fs:allow-watch",
      "allow": [
        { "path": "$APPCONFIG/**" },
        { "path": "$APPDATA/**" },
        { "path": "$HOME_DIR/**" }
      ]
    },
    "fs:allow-unwatch"
  ]
}
